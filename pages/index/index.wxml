<!--index.wxml-->
<view class="page">
  <view>
    <text>欢迎你来到“不读会死”！</text>
    <view class="group">
        <icon wx:for="{{iconSize}}" wx:key="{{index}}" type="success" size="{{item}}"/>
    </view>

    <view class="group">
        <icon wx:for="{{iconType}}" wx:key="{{index}}" type="{{item}}" size="40"/>
    </view>


    <view class="group">
        <icon  wx:for="{{iconColor}}" wx:key="{{index}}" type="success" size="40" color="{{item}}"/>
    </view>
  </view>
</view>
